<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.brand.musicappy">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".PortugalActivity"
            android:label="@string/portugal" />
        <activity
            android:name=".DepecheActivity"
            android:label="@string/depeche_mode" />
        <activity
            android:name=".AltjActivity"
            android:label="@string/altj" />
        <activity
            android:name=".GrimesActivity"
            android:label="@string/grimes" />
        <activity
            android:name=".NowPlayingActivity"
            android:label="@string/now_playing"
            android:parentActivityName=".MainActivity"
            android:theme="@style/NoActionBarTheme"
            />

    </application>

</manifest>